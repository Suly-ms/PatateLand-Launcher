{
    "name": "PatateLand",
    "productName": "PatateLand",
    "version": "1.2.3",
    "description": "Launcher Minecraft modd√© pour le serveur PatateLand",
    "main": "src/app.js",
    "author": {
        "name": "Suly-ms, Kales, Inc.",
        "email": "/",
        "url": "https://github.com/Suly-ms"
    },
    "url": "https://filelauncher.patateland.wstr.fr",
    "scripts": {
        "start": "cross-env-shell NODE_ENV=dev electron .",
        "dev": "cross-env-shell DEV_TOOL=open nodemon --exec npm start",
        "build": "node build.js --obf=true --build=platform",
        "icon": "node build.js --icon"
    },
    "license": "CCANC",
    "nodemonConfig": {
        "ext": "js,html,css",
        "ignore": "**/test"
    },
    "build": {
        "appId": "fr.patateland.launcher",
        "productName": "PatateLand",
        "win": {
            "icon": "src/assets/images/icon/icon.ico",
            "target": "nsis"
        },
        "mac": {
            "icon": "src/assets/images/icon/icon.icns"
        },
        "linux": {
            "icon": "src/assets/images/icon/icon.png"
        }
    },
    "dependencies": {
        "dotenv": "^17.3.1",
        "electron-store": "^8.2.0",
        "electron-updater": "^6.7.3",
        "minecraft-java-core": "^4.2.5",
        "node-fetch": "^2.7.0",
        "node-notifier": "^10.0.1",
        "rcon-client": "^4.2.5",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "cross-env": "^10.1.0",
        "electron": "^40.0.0",
        "electron-builder": "^26.0.12",
        "javascript-obfuscator": "^5.1.0",
        "jimp": "^1.6.0",
        "nodemon": "^3.1.11",
        "png2icons": "^2.0.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Suly-ms/PatateLand-Launcher.git"
    }
}
